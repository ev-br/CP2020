    "    print(\"nodes =\", i, \"  Error =\", np.linalg.norm(f(xx) - f_unif(xx, y4)))\n",
    "    \n",
    "plt.plot(xx, abs(f_unif(xx, y4)- f(xx)))\n",
    "plt.ylabel(\"Unifrom grid error\")\n",
    "plt.show()"
