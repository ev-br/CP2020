    "def f_calc(n):\n",
    "    x, w = sp.roots_sh_legendre(n)\n",
    "    y = np.linalg.solve(1/2*w-np.eye(n), -np.sin(np.pi*x))\n",
    "    return x, y\n",
    "\n",
    "def f(x):\n",
    "    return np.sin(np.pi*x)+2/np.pi\n",
    "\n",
    "for i in range(1,15):\n",
    "    x3, y = f_calc(i)\n",
    "    error = np.linalg.norm(y - f(x3))\n",
    "    print(\"nodes =\",i, \" Error =\", error)"
   ]
